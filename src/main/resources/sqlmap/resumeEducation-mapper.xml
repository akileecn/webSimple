<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.aki.dao.ResumeEducationMapper">
	<cache/>
	<sql id="whereSql">
		<where>
			<if test="id!=null">
			id=#{id}
			</if>
			<if test="remsumeId!=null">
			and remsume_id=#{remsumeId}
			</if>
		</where>
	</sql>
	
	<sql id="selectSql">
		select
			id
			,remsume_id as remsumeId
			,school_name as schoolName
			,major
			,graduate_type as graduateType
			,degree
			,education
			,learn_type as learnType
			,begin_date as beginDate
			,end_date as endDate
			,school_type as schoolType
			,has_been_cadre as hasBeenCadre
			,grade_rank as gradeRank
		from resume_education
	</sql>
	
	<!-- 获得 -->
	<select id="get" parameterType="resumeEducation" resultType="resumeEducation">
		<include refid="selectSql"/>
		<include refid="whereSql"/>
	</select>
	
	<!-- 查询 -->
	<select id="getList" parameterType="resumeEducation" resultType="resumeEducation">
		<include refid="selectSql"/>
		<include refid="whereSql"/>
	</select>
	
	<!-- 保存 -->
	<insert id="save" parameterType="resumeEducation">
		insert into resume_education(
		<trim prefix="" prefixOverrides=",">
			<if test="id!=null">
			id
			</if>
			<if test="remsumeId!=null">
			,remsume_id
			</if>
			<if test="schoolName!=null">
			,school_name
