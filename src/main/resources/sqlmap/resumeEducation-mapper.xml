			</if>
			<if test="endDate!=null">
			,end_date
			</if>
			<if test="schoolType!=null">
			,school_type
			</if>
			<if test="hasBeenCadre!=null">
			,has_been_cadre
			</if>
			<if test="gradeRank!=null">
			,grade_rank
			</if>
		</trim>
		)values(
		<trim prefix="" prefixOverrides=",">
			<if test="id!=null">
